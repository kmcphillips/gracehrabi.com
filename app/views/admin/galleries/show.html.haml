%h1= @gallery_name

- @images.each do |image|
  .fake_thumb{:style => "background: #7D7B56 url('#{image.thumb}') no-repeat left top;"}
    .delete= destroy_entity_image admin_images_path

.post
  %h1 Upload New Image
  = render :partial => 'admin/galleries/form'

.image_links
  = index_entity_image admin_galleries_path, "Back to Galleries Index"

